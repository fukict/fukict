{
  "name": "@fukict/i18n",
  "version": "0.1.6",
  "description": "Type-safe internationalization library for Fukict framework",
  "keywords": [
    "fukict",
    "i18n",
    "internationalization",
    "localization",
    "translation",
    "reactive"
  ],
  "bugs": {
    "url": "https://github.com/fukict/fukict/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fukict/fukict.git",
    "directory": "packages/i18n"
  },
  "license": "MIT",
  "author": "Fukict Team",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsx ../../scripts/build-package.ts --pkg-name i18n --no-watch",
    "dev": "tsx ../../scripts/build-package.ts --pkg-name i18n --watch",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
