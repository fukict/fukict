{
  "name": "@vanilla-dom/babel-plugin",
  "version": "0.1.0",
  "description": "Babel plugin for @vanilla-dom JSX compilation",
  "keywords": [
    "babel-plugin",
    "jsx",
    "vanilla-dom",
    "compiler"
  ],
  "license": "MIT",
  "author": "xiaoyown",
  "repository": {
    "type": "git",
    "url": "https://github.com/vanilla-dom/vanilla-dom.git",
    "directory": "packages/babel-plugin"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@types/babel__core": "^7.20.0",
    "@types/babel__traverse": "^7.20.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  }
}
