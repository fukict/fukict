{
  "name": "fukict",
  "version": "0.0.0",
  "private": true,
  "description": "专注于性能场景的轻量级 DOM 渲染库",
  "keywords": [
    "dom",
    "jsx",
    "vanilla",
    "typescript",
    "rendering",
    "performance"
  ],
  "homepage": "https://github.com/fukict/fukict#readme",
  "bugs": {
    "url": "https://github.com/fukict/fukict/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fukict/fukict.git"
  },
  "license": "MIT",
  "author": "xiaoyown <xiaoyown@example.com>",
  "type": "module",
  "scripts": {
    "build": "pnpm clean && pnpm extract-metadata && pnpm build:pkg && pnpm build:demos",
    "build:demos": "pnpm --filter './demos/*' build",
    "build:pkg": "pnpm extract-metadata && tsx scripts/build-package.ts --all --no-watch",
    "build:pkg:watch": "tsx scripts/build-package.ts --all --watch",
    "changeset": "changeset",
    "changeset:alpha": "changeset version --snapshot alpha",
    "changeset:beta": "changeset version --snapshot beta",
    "changeset:publish": "changeset publish",
    "changeset:release": "pnpm build && changeset publish",
    "changeset:version": "changeset version",
    "clean": "pnpm -r clean",
    "clear": "premove **/node_modules && premove pnpm-lock.yaml",
    "extract-metadata": "tsx scripts/extract-metadata.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "publish": "tsx scripts/release.ts",
    "release": "tsx scripts/release.ts",
    "test": "pnpm -r test",
    "version:alpha": "changeset version --snapshot alpha && changeset publish --tag alpha",
    "version:beta": "changeset version --snapshot beta && changeset publish --tag beta",
    "version:rc": "changeset version --snapshot rc && changeset publish --tag rc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@eslint/js": "^9.28.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "premove": "^4.0.0",
    "prettier": "3.4.2",
    "prettier-plugin-packagejson": "2.5.6",
    "prettier-plugin-sort-json": "4.0.0",
    "tsdown": "^0.12.9",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "yaml": "^2.6.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@7.33.7+sha512.7afe2410642b39c698df46da4ce5c7231a5cce360698d69f8cf8f42257d40b3e43f231053b07b8de849fd4ffbf4a71ff57b835137777a352388f3d3da747200e"
}
