{
  "name": "fukict",
  "version": "0.0.0",
  "private": true,
  "description": "专注于性能场景的轻量级 DOM 渲染库",
  "keywords": [
    "dom",
    "jsx",
    "vanilla",
    "typescript",
    "rendering",
    "performance"
  ],
  "homepage": "https://github.com/fukict/fukict#readme",
  "bugs": {
    "url": "https://github.com/fukict/fukict/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fukict/fukict.git"
  },
  "license": "MIT",
  "author": "xiaoyown <xiaoyown@example.com>",
  "type": "module",
  "scripts": {
    "build": "pnpm clean && pnpm extract-metadata && tsx scripts/build-package.ts --all --no-watch",
    "build:watch": "tsx scripts/build-package.ts --all --watch",
    "changeset": "changeset",
    "changeset:alpha": "changeset version --snapshot alpha",
    "changeset:beta": "changeset version --snapshot beta",
    "changeset:publish": "changeset publish",
    "changeset:release": "pnpm build && changeset publish",
    "changeset:version": "changeset version",
    "clean": "premove packages/*/dist",
    "clear": "premove **/node_modules && premove pnpm-lock.yaml",
    "dev:no-build": "npx http-server -p 8080 -c-1 --cors",
    "extract-metadata": "tsx scripts/extract-metadata.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit && pnpm -r exec tsc --noEmit",
    "prepare": "husky",
    "publish": "tsx scripts/release.ts",
    "release": "tsx scripts/release.ts",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck",
    "version:alpha": "changeset version --snapshot alpha && changeset publish --tag alpha",
    "version:beta": "changeset version --snapshot beta && changeset publish --tag beta",
    "version:rc": "changeset version --snapshot rc && changeset publish --tag rc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@eslint/js": "^9.28.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "premove": "^4.0.0",
    "prettier": "3.4.2",
    "prettier-plugin-packagejson": "2.5.6",
    "prettier-plugin-sort-json": "4.0.0",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "yaml": "^2.6.1"
  },
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  }
}
